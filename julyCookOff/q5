#include<bits/stdc++.h>
#define ll long long int 
#define fio ios::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL)
using namespace std;

void solve(){
  ll n;
  cin>>n;

  if(n==3)cout<<-1<<endl;
  else{
  	cout<<"1 3 4 2 ";
  	for(int i=5;i<=n;i++){
  	    cout<<i<<" ";
  	}
  	cout<<endl;
  }

}

int main(){
    ll t;
  cin>>t;
    while(t--){
        solve();
    }
    return 0;
}
