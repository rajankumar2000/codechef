#include<bits/stdc++.h>
#define ll long long int 
#define fio ios::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL)
using namespace std;

void solve(){
  ll n;
  cin>>n;

  int i=1,j=n;
  while(i<j){
  	cout<<j<<" "<<i<<" ";
  	i++;
  	j--;
  }
  if(i==j)cout<<i;
  cout<<endl;

}

int main(){
    ll t;
  cin>>t;
    while(t--){
        solve();
    }
    return 0;
}
